{
  "kind": "collectionType",
  "collectionName": "notificaciones",
  "info": {
    "singularName": "notificacion",
    "pluralName": "notificaciones",
    "displayName": "Notificaciones",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "municipio": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::municipio.municipio"
    },
    "perfil_hogar": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::perfil-hogar.perfil-hogar"
    },
    "ciclo_vital": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ciclo-vital.ciclo-vital"
    },
    "asunto": {
      "type": "string",
      "required": true,
      "maxLength": 300
    },
    "Mensaje": {
      "type": "richtext",
      "required": true,
      "maxLength": 400
    },
    "enviado": {
      "type": "boolean",
      "default": false
    },
    "categoria": {
      "type": "enumeration",
      "enum": [
        "prosperidad",
        "empleo",
        "interes"
      ],
      "required": false,
      "default": "prosperidad"
    },
    "integrantes": {
      "type": "richtext"
    },
    "fechaEnvio": {
      "type": "datetime"
    },
    "cantidadEnvio": {
      "type": "biginteger"
    },
    "fechaFin": {
      "type": "date"
    },
    "linkExterno": {
      "type": "string"
    },
    "BotonAlerta": {
      "type": "string"
    }
  }
}
