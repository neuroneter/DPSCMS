{
  "kind": "collectionType",
  "collectionName": "encuestas",
  "info": {
    "singularName": "encuesta",
    "pluralName": "encuestas",
    "displayName": "Encuestas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Pregunta": {
